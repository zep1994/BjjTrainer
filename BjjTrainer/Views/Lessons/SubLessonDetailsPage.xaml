<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BjjTrainer.Views.Lessons.SubLessonDetailsPage">
    <ContentPage.Content>
        <StackLayout Padding="10">
            <Label Text="{Binding SubLessonDetails.Title}" FontSize="24" />
            <Label Text="Content" FontSize="18" />
            <Label Text="{Binding SubLessonDetails.Content}" />
            <Label Text="Notes" FontSize="18" />
            <Label Text="{Binding SubLessonDetails.Notes}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
